   "source": [
    "## Sol 3\n",
    "\n",
    "SELECT FIRST_NAME FROM Worker WHERE w SALARY = (SELECT MAX(SALARY) FROM Worker WHERE DEPARTMENT = w.DEPARTMENT)"
